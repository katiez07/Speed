func fib ;' n ';
./
	if ;' == ;' n 0 '; ';
	./
		0
	/.
	else if ;' == ;' n 1 '; ';
	./
		1
	/.
	else
	./
		+ ;' fib ;' - ;' n 2 '; ';  fib ;' - ;' n 1 ';'; ';
	/.
/.

